<h1>Listing Suppliers_Contact_Details</h1>

<table>
  <tr>
    <th>User</th>
    <th>Contact detail</th>
    <th>Contact detail type</th>
  </tr>

<% @suppliers_contact_details.each do |suppliers_contact_detail| %>
  <tr>
    <td><%=h suppliers_contact_detail.user_id %></td>
    <td><%=h suppliers_contact_detail.contact_detail_id %></td>
    <td><%=h suppliers_contact_detail.contact_detail_type_id %></td>
    <td><%= link_to 'Show', suppliers_contact_detail %></td>
    <td><%= link_to 'Edit', edit_suppliers_contact_detail_path(suppliers_contact_detail) %></td>
    <td><%= link_to 'Destroy', suppliers_contact_detail, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New suppliers_contact_detail', new_suppliers_contact_detail_path %>