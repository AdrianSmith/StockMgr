<h1>Listing Users_Contact_Details</h1>

<table>
  <tr>
    <th>User</th>
    <th>Contact detail</th>
    <th>Contact detail type</th>
  </tr>

<% @users_contact_details.each do |users_contact_detail| %>
  <tr>
    <td><%=h users_contact_detail.user_id %></td>
    <td><%=h users_contact_detail.contact_detail_id %></td>
    <td><%=h users_contact_detail.contact_detail_type_id %></td>
    <td><%= link_to 'Show', users_contact_detail %></td>
    <td><%= link_to 'Edit', edit_users_contact_detail_path(users_contact_detail) %></td>
    <td><%= link_to 'Destroy', users_contact_detail, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New users_contact_detail', new_users_contact_detail_path %>