<h1>Listing Physical_Forms</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
  </tr>

<% @physical_forms.each do |physical_form| %>
  <tr>
    <td><%=h physical_form.name %></td>
    <td><%=h physical_form.description %></td>
    <td><%= link_to 'Show', physical_form %></td>
    <td><%= link_to 'Edit', edit_physical_form_path(physical_form) %></td>
    <td><%= link_to 'Destroy', physical_form, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New physical_form', new_physical_form_path %>