<h1>Listing Purchase_Order_States</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
  </tr>

<% @purchase_order_states.each do |purchase_order_state| %>
  <tr>
    <td><%=h purchase_order_state.name %></td>
    <td><%=h purchase_order_state.description %></td>
    <td><%= link_to 'Show', purchase_order_state %></td>
    <td><%= link_to 'Edit', edit_purchase_order_state_path(purchase_order_state) %></td>
    <td><%= link_to 'Destroy', purchase_order_state, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New purchase_order_state', new_purchase_order_state_path %>